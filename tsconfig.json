{
  "include": ["api/**/*", "cypress/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "types": ["cypress", "mocha", "node", "chai", "dotenv", "axios"],
    "paths": {
      "*": ["node_modules/*"]
    },
    "moduleResolution": "node",
    "typeRoots": ["./node_modules/@types"]
  }
}
